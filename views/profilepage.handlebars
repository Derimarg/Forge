{{>nav-active-panel}}
<main>

    <!-- SCROLL -->
    <div class="scroll" data-rate=".3" data-direction="vertical">
        <div class="wrapper">
            <div class="container">
                <div class="container">

                    {{>left-panel}}

                    <div class="middle-panel">
                        {{#each users as |user|}}
                        <div class="post">
                            <div class="input-update">{{name}}</div>
                            <div class="input-update">{{username}}</div>
                            <div class="input-update">{{email}}</div>
                            <div class="input-update">{{password}}</div>
                            {{!-- <div class="action">
                                <a href="/profile/user/{{id}}"><button class="btn-up">Edit Profile</button></a>
                            </div> --}}
                        </div>

                        <div class="action">
                            <a href="profile/edit/{{user.id}}"><button class="btn-up">Edit Profile</button></a>
                        </div>
                        {{/each}}

                    </div>

                    {{>right-panel}}
                </div>
            </div>
        </div>

</main>